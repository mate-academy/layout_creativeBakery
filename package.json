{
  "name": "creative-bakery",
  "version": "1.0.0",
"scripts": {
    "start": "npm run dev",
    "dev": "parcel src/index.html --open --dist-dir dist",
    "build": "parcel build src/index.html --dist-dir dist --public-url ./",
    "lint": "npm run style-format && npm run format",
    "deploy": "rimraf dist && npm run build && gh-pages -d dist"
  },
  "parcel": {
    "build": {
      "publicUrl": "./"
    }
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.16.0",
    "gh-pages": "^6.3.0",
    "parcel": "^2.12.0",
    "rimraf": "^5.0.10"
  }
}
